# Copy this file to .env and fill in your values
# After creating .env, restart the React dev server (npm start)

# Backend API URL (NO trailing slash!)
# For development:
REACT_APP_BACKEND_URL=http://localhost:5001
# For production, use your domain:
# REACT_APP_BACKEND_URL=https://your-domain.com

# Mapbox Token (required for map features)
# Get a free token at: https://account.mapbox.com/auth/signup/
# After signup, find your token at: https://account.mapbox.com/access-tokens/
# Your token should start with "pk."
REACT_APP_MAPBOX_TOKEN=pk.YOUR_TOKEN_HERE

# Optional: Custom embedding service URL
# EMBEDDING_SERVICE_URL=http://localhost:5002

